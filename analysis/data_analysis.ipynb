   "source": [
    "## 6. Recommendation Matrix"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Create recommendation matrix based on different priorities\n",
    "print(\"üéØ RECOMMENDATION MATRIX FOR DIFFERENT USE CASES\")\n",
    "print(\"=\" * 90)\n",
    "\n",
    "use_cases = [\n",
    "    {\n",
    "        'name': 'New Web Application',\n",
    "        'priority': 'Maximum security with modern practices',\n",
    "        'weights': {'security': 0.6, 'performance': 0.2, 'scalability': 0.2}\n",
    "    },\n",
    "    {\n",
    "        'name': 'High-Performance API',\n",
    "        'priority': 'Low latency with good security',\n",
    "        'weights': {'security': 0.3, 'performance': 0.5, 'scalability': 0.2}\n",
    "    },\n",
    "    {\n",
    "        'name': 'Legacy System Migration',\n",
    "        'priority': 'Easy implementation with gradual improvement',\n",
    "        'weights': {'security': 0.4, 'performance': 0.3, 'implementation': 0.3}\n",
    "    },\n",
    "    {\n",
    "        'name': 'Rapid Prototyping',\n",
    "        'priority': 'Development speed with reasonable security',\n",
    "        'weights': {'security': 0.3, 'implementation': 0.5, 'maintenance': 0.2}\n",
    "    },\n",
    "    {\n",
    "        'name': 'Compliance-Driven Project',\n",
    "        'priority': 'Maximum audit compliance and documentation',\n",
    "        'weights': {'security': 0.7, 'maintenance': 0.2, 'scalability': 0.1}\n",
    "    }\n",
    "]\n",
    "\n",
    "# Calculate scores for each use case\n",
    "recommendations = {}\n",
    "\n",
    "for use_case in use_cases:\n",
    "    scores = {}\n",
    "    \n",
    "    for tech in df['Technique']:\n",
    "        if tech != 'No Protection':\n",
    "            row = df[df['Technique'] == tech].iloc[0]\n",
    "            \n",
    "            # Calculate weighted score based on use case priorities\n",
    "            score = 0\n",
    "            weights = use_case['weights']\n",
    "            \n",
    "            if 'security' in weights:\n",
    "                score += row['SecurityScore'] * weights['security']\n",
    "            if 'performance' in weights:\n",
    "                score += (100 - ((row['AvgResponseTime'] - 12) / 6 * 100)) * weights['performance'] / 100\n",
    "            if 'scalability' in weights:\n",
    "                score += row['ScalabilityScore'] * 10 * weights['scalability']\n",
    "            if 'implementation' in weights:\n",
    "                score += (10 - row['ImplementationDifficulty']) * 10 * weights['implementation']\n",
    "            if 'maintenance' in weights:\n",
    "                score += (10 - row['MaintenanceCost']) * 10 * weights['maintenance']\n",
    "            \n",
    "            scores[tech] = score\n",
    "    \n",
    "    # Get top recommendation\n",
    "    top_tech = max(scores, key=scores.get)\n",
