   "metadata": {},
   "source": [
    "## 5. Multi-dimensional Radar Chart Analysis"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "from math import pi\n",
    "\n",
    "# Prepare data for radar chart\n",
    "categories = ['Security', 'Performance', 'Scalability', 'Implementation', 'Maintenance']\n",
    "N = len(categories)\n",
    "\n",
    "# Normalize all values to 0-10 scale for radar chart\n",
    "radar_data = {}\n",
    "for tech in df['Technique']:\n",
    "    if tech != 'No Protection':\n",
    "        row = df[df['Technique'] == tech].iloc[0]\n",
    "        \n",
    "        # Convert to 0-10 scale\n",
    "        radar_data[tech] = [\n",
    "            row['SecurityScore'] / 10,                    # Security (0-10)\n",
    "            10 - (row['AvgResponseTime'] - 12) / 2,       # Performance (inverted, 0-10)\n",
    "            row['ScalabilityScore'],                      # Scalability (0-10)\n",
    "            10 - row['ImplementationDifficulty'],         # Implementation ease (0-10)\n",
    "            10 - row['MaintenanceCost']                   # Maintenance ease (0-10)\n",
    "        ]\n",
    "\n",
    "# Create radar chart\n",
    "angles = [n / float(N) * 2 * pi for n in range(N)]\n",
    "angles += angles[:1]\n",
    "\n",
    "fig, ax = plt.subplots(figsize=(12, 12), subplot_kw=dict(projection='polar'))\n",
    "\n",
    # Set radar chart properties\n",
    "ax.set_theta_offset(pi / 2)\n",
    "ax.set_theta_direction(-1)\n",
    "plt.xticks(angles[:-1], categories, fontsize=13, fontweight='bold')\n",
    "ax.set_rlabel_position(0)\n",
    "plt.yticks([2, 4, 6, 8, 10], [\"2\", \"4\", \"6\", \"8\", \"10\"], color=\"grey\", size=10)\n",
    "plt.ylim(0, 10)\n",
    "\n",
    # Colors for different techniques\n",
    "colors = ['#FF6B6B', '#4ECDC4', '#45B7D1', '#96CEB4', '#FFEAA7']\n",
    "\n",
    # Plot each technique\n",
    "for idx, (tech, values) in enumerate(radar_data.items()):\n",
    "    values_radar = values\n",
    "    values_radar += values_radar[:1]\n",
    "    \n",
    "    ax.plot(angles, values_radar, linewidth=3, linestyle='solid', \n",
    "            label=tech, color=colors[idx])\n",
    "    ax.fill(angles, values_radar, alpha=0.25, color=colors[idx])\n",
    "\n",
    # Add title and legend\n",
    "plt.title('Multi-dimensional Comparison of SQL Injection Prevention Techniques', \n",
    "          size=16, fontweight='bold', y=1.08)\n",
    "plt.legend(loc='upper right', bbox_to_anchor=(1.3, 1.0), fontsize=11, frameon=True)\n",
    "\n",
    # Add grid\n",
    "ax.grid(True, alpha=0.3)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.savefig('../docs/radar_chart_comparison.png', dpi=300, bbox_inches='tight')\n",
    "plt.show()"
   ]
  },
