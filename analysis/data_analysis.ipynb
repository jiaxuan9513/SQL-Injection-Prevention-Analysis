    "\n",
    "ax.legend(scatterpoints=1, frameon=True, labelspacing=1.5, title='Implementation Difficulty',\n",
    "          loc='lower right', fontsize=10)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.savefig('../docs/security_performance_tradeoff.png', dpi=300, bbox_inches='tight')\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Calculate trade-off scores\n",
    "print(\"‚öñÔ∏è SECURITY-PERFORMANCE TRADE-OFF ANALYSIS\")\n",
    "print(\"=\" * 70)\n",
    "\n",
    "tradeoff_df = df[df['Technique'] != 'No Protection'].copy()\n",
    "\n",
    "# Normalize scores (0-100, higher is better)\n",
    "tradeoff_df['NormSecurity'] = tradeoff_df['SecurityScore']  # Already 0-100\n",
    "tradeoff_df['NormPerformance'] = 100 - ((tradeoff_df['AvgResponseTime'] - tradeoff_df['AvgResponseTime'].min()) / \n",
    "                                        (tradeoff_df['AvgResponseTime'].max() - tradeoff_df['AvgResponseTime'].min()) * 100)\n",
    "tradeoff_df['NormScalability'] = tradeoff_df['ScalabilityScore'] * 10  # Convert 0-10 to 0-100\n",
    "\n",
    "# Calculate weighted overall score\n",
    "weights = {'security': 0.5, 'performance': 0.3, 'scalability': 0.2}\n",
    "tradeoff_df['OverallScore'] = (\n",
    "    tradeoff_df['NormSecurity'] * weights['security'] +\n",
    "    tradeoff_df['NormPerformance'] * weights['performance'] +\n",
    "    tradeoff_df['NormScalability'] * weights['scalability']\n",
    ")\n",
    "\n",
    "# Sort by overall score\n",
    "tradeoff_df = tradeoff_df.sort_values('OverallScore', ascending=False)\n",
    "\n",
    "# Display results\n",
    "print(f\"{'Technique':25} | {'Security':>8} | {'Performance':>10} | {'Scalability':>10} | {'Overall':>8}\")\n",
    "print(\"-\" * 70)\n",
    "\n",
    "for idx, row in tradeoff_df.iterrows():\n",
    "    medal = \"ü•á\" if idx == 0 else \"ü•à\" if idx == 1 else \"ü•â\" if idx == 2 else \"  \"\n",
    "    \n",
    "    print(f\"{medal}{row['Technique']:23} | \"\n",
    "          f\"{row['NormSecurity']:>7.1f} | \"\n",
    "          f\"{row['NormPerformance']:>9.1f} | \"\n",
    "          f\"{row['NormScalability']:>9.1f} | \"\n",
    "          f\"{row['OverallScore']:>7.1f}\")\n",
    "\n",
    "print(\"\\nüìä Weighting: Security (50%), Performance (30%), Scalability (20%)\")"
   ]
  },
