   "source": [
    "# Calculate performance overhead compared to no protection\n",
    "baseline = df[df['Technique'] == 'No Protection'].iloc[0]\n",
    "\n",
    "df['ResponseOverhead'] = ((df['AvgResponseTime'] - baseline['AvgResponseTime']) / \n",
    "                          baseline['AvgResponseTime'] * 100)\n",
    "df['CPUOverhead'] = df['CPUUsage'] - baseline['CPUUsage']\n",
    "df['ThroughputLoss'] = ((baseline['QueryThroughput'] - df['QueryThroughput']) / \n",
    "                        baseline['QueryThroughput'] * 100)\n",
    "\n",
    "# Create performance comparison table\n",
    "print(\"‚ö° PERFORMANCE OVERHEAD COMPARISON (vs No Protection)\")\n",
    "print(\"=\" * 80)\n",
    "print(f\"Baseline (No Protection): Response Time: {baseline['AvgResponseTime']}ms, \"\n",
    "      f\"CPU: {baseline['CPUUsage']}%, Throughput: {baseline['QueryThroughput']} req/sec\")\n",
    "print()\n",
    "\n",
    "performance_df = df[df['Technique'] != 'No Protection'].copy()\n",
    "performance_df = performance_df.sort_values('ResponseOverhead')\n",
    "\n",
    "print(f\"{'Technique':25} | {'Response':>10} | {'CPU':>8} | {'Throughput':>12} | {'Perf Score':>10}\")\n",
    "print(\"-\" * 80)\n",
    "\n",
    "for _, row in performance_df.iterrows():\n",
    "    # Calculate performance score (lower overhead = better)\n",
    "    perf_score = (100 - min(row['ResponseOverhead'], 100)) * 0.4 + \\\n",
    "                 (100 - min(row['CPUOverhead'], 50)) * 0.3 + \\\n",
    "                 (100 - min(row['ThroughputLoss'], 100)) * 0.3\n",
    "    \n",
    "    print(f\"{row['Technique']:25} | \"\n",
    "          f\"{row['ResponseOverhead']:>+6.1f}% | \"\n",
    "          f\"{row['CPUOverhead']:>+6.1f}% | \"\n",
    "          f\"{row['ThroughputLoss']:>+9.1f}% | \"\n",
    "          f\"{perf_score:>9.1f}\")"
   ]
  },
