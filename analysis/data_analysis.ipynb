import pandas as pd
import matplotlib.pyplot as plt
import json
import os

print("ğŸ” SQL Injection Prevention Analysis")
print("=" * 50)

# 1. Load data
df = pd.read_csv('data/experimental_results.csv')
print(f"âœ… Data loaded: {len(df)} techniques")

# 2. Calculate security scores
security_metrics = ['AuthBypass', 'DataExtraction', 'DBManipulation', 'SystemCompromise']
df['SecurityScore'] = df[security_metrics].mean(axis=1)

# 3. Display results
print("\nğŸ“Š SECURITY RANKING:")
print("-" * 40)
df_sorted = df.sort_values('SecurityScore', ascending=False)

medals = ["ğŸ¥‡", "ğŸ¥ˆ", "ğŸ¥‰", "4.", "5.", "6."]
for i, (_, row) in enumerate(df_sorted.iterrows()):
    print(f"{medals[i]} {row['Technique']:30} {row['SecurityScore']:6.1f}%")

# 4. Create simple chart
plt.figure(figsize=(10, 6))
colors = ['#2E86AB', '#A23B72', '#F18F01', '#C73E1D', '#6A994E', '#495057']
bars = plt.bar(df_sorted['Technique'], df_sorted['SecurityScore'], color=colors)

plt.title('SQL Injection Prevention: Security Effectiveness', fontsize=14, fontweight='bold')
plt.ylabel('Security Score (%)')
plt.xticks(rotation=45)
plt.ylim(0, 105)
plt.grid(axis='y', alpha=0.3)

# Add value labels
for bar in bars:
    height = bar.get_height()
    plt.text(bar.get_x() + bar.get_width()/2., height + 1,
             f'{height:.1f}%', ha='center', va='bottom', fontsize=9)

plt.tight_layout()

# 5. Save outputs
os.makedirs('docs', exist_ok=True)
plt.savefig('docs/security_results.png', dpi=300)
print(f"\nâœ… Chart saved: docs/security_results.png")

# 6. Save JSON results
results = {
    'security_ranking': df_sorted[['Technique', 'SecurityScore']].to_dict('records'),
    'key_findings': {
        'best_overall': df_sorted.iloc[0]['Technique'],
        'best_security': df_sorted.iloc[0]['Technique']
    }
}

with open('data/analysis_results.json', 'w') as f:
    json.dump(results, f, indent=2)

print("âœ… Results saved: data/analysis_results.json")
print("\n" + "=" * 50)
print("âœ… Analysis complete!")
